generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Entry {
  id        Int      @id @default(autoincrement())
  title     String
  type      String
  director  String
  budget    Float
  location  String
  duration  String
  year      Int
  createdAt DateTime @default(now())
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
  name     String
  createdAt DateTime @default(now())
}